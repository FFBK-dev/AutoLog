# Environment Configuration for FileMaker Backend
# Copy this file to .env and fill in your actual values

# FileMaker Server Configuration
FILEMAKER_SERVER=your.filemaker.server.ip
FILEMAKER_USERNAME=your_filemaker_username
FILEMAKER_PASSWORD=your_filemaker_password

# API Authentication
FM_AUTOMATION_KEY=your_api_key_here

# OpenAI API Keys (stored in FileMaker - these are field names)
# SystemGlobals_AutoLog_OpenAI_API_Key_1=your_openai_key_1
# SystemGlobals_AutoLog_OpenAI_API_Key_2=your_openai_key_2
# SystemGlobals_AutoLog_OpenAI_API_Key_3=your_openai_key_3
# SystemGlobals_AutoLog_OpenAI_API_Key_4=your_openai_key_4
# SystemGlobals_AutoLog_OpenAI_API_Key_5=your_openai_key_5

# Notion API Configuration (for Music AutoLog workflow)
NOTION_KEY=your_notion_api_key_here
NOTION_DB_ID=your_notion_database_id_here

# Development Settings
AUTOLOG_DEBUG=false 