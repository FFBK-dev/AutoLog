<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" 
"http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.florentine.keyframes</string>

    <key>ProgramArguments</key>
    <array>
        <string>/usr/bin/python3</string>
        <string>/Users/admin/Documents/Github/Keyframes/keyframes.py</string>
    </array>

    <key>StartInterval</key>
    <integer>300</integer> <!-- Every 5 minutes -->

    <key>RunAtLoad</key>
    <true/>

    <key>StandardOutPath</key>
    <string>/tmp/keyframes.log</string>

    <key>StandardErrorPath</key>
    <string>/tmp/keyframes.err</string>
</dict>
</plist>